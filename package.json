{
  "name": "openui5-sample-app",
  "version": "0.2.0",
  "description": "Sample of an OpenUI5 app",
  "private": true,
  "scripts": {
    "start": "ui5 serve",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "rimraf coverage && karma start karma-ci.conf.js",
    "watch": "npm run karma",
    "test": "npm run lint && npm run karma-ci",
    "build": "ui5 build -a --clean-dest",
    "build-self-contained": "ui5 build self-contained -a --clean-dest",
    "serve-dist": "ws --compress -d dist"
  },
  "dependencies": {
    "@openui5/sap.m": "^1.73.1",
    "@openui5/sap.ui.core": "^1.73.1",
    "@openui5/themelib_sap_fiori_3": "^1.73.1"
  },
  "devDependencies": {
    "@ui5/cli": "^1.12.2",
    "@ui5/webcomponents": "^1.0.0-rc.7",
    "eslint": "^5.16.0",
    "grunt": "^1.0.4",
    "gulp": "^4.0.2",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-ui5": "^1.1.0",
    "local-web-server": "^3.0.7",
    "rimraf": "^3.0.0"
  },
  "main": "karma-ci.conf.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpielaszkiewicz/ui5_app.git"
  },
  "engines": {
    "node": "12.18.0"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tpielaszkiewicz/ui5_app/issues"
  },
  "homepage": "https://github.com/tpielaszkiewicz/ui5_app#readme"
}
